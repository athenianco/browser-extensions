{
    "name": "Delivery Pipeline Metrics for GitHub",
    "author": "Athenian",
    "homepage_url": "https://github.com/athenianco/github-browser-extension",
    "version": "1.2",
    "description": "Adds another section in the GitHub PR view to present the pipeline stage timings calculated by Athenian.",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "settings.html",
        "default_icon": {
            "16": "16.png",
            "32": "32.png",
            "48": "48.png",
            "128": "128.png"
        }
    },
    "icons": {
        "16": "16.png",
        "32": "32.png",
        "48": "48.png",
        "128": "128.png"
    },
    "options_page": "settings.html",
    "permissions": ["storage"],
    "content_scripts": [{
        "matches": [
          "https://github.com/*/*/pull/*"
        ],
        "js": ["insert_sidebar_item.js"]
    }],
    "content_security_policy": "script-src 'self' 'sha256-4m6kinKizsKFzXHN3Wnlosb5T1E0k1DgbG/jRKJ2p8w='; object-src 'self'"
}
